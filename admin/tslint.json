{
  "defaultSeverity": "none",
  "extends": [
    "tslint:recommended"
  ],
  "linterOptions": {
    "exclude": [
      "node_modules/**"
    ]
  },
  "rules": {
    "trailing-comma": [
      true,
      {
        "singleline": "never",
        "multiline": {
          "objects": "ignore",
          "arrays": "ignore",
          "functions": "ignore",
          "typeLiterals": "ignore"
        }
      }
    ],
    "no-empty": false,
    "no-angle-bracket-type-assertion": false,
    "no-trailing-whitespace": true,
    "comment-format": [
      true,
      "check-space"
    ],
    "curly": true,
    "indent": false,
    "ordered-imports": false,
    "no-console": false,
    "arrow-parens": false,
    "interface-name": false,
    "no-null-keyword": false,
    "no-consecutive-blank-lines": false,
    "object-literal-sort-keys": false,
    "quotemark": false,
    "semicolon": false
  }
}